{
    "env": {
        "es6": true,
        "browser": true,
        "webextensions": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        // Additional, per-project rules
        "no-unused-vars": "off",
        "no-undef": "off"
    },
    "globals": {
        // Global constants
        "tf": false
    }
  }